<Page x:Class="Intāfēsu.Pages.About"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Intāfēsu.Pages"
      xmlns:intāfēsu="clr-namespace:Intāfēsu"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="About">
	<Page.Resources>
		<intāfēsu:ViewModelMainWindow x:Key="ViewModelMainWindow"/>
	</Page.Resources>
	<StackPanel DataContext="{Binding Source={StaticResource ViewModelMainWindow}}">
		<TextBlock Text="{Binding CurrentVersion}"></TextBlock>
    </StackPanel>
</Page>
