<Window x:Class="callUnrepairGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="125" Width="200" Background="#FF462674" Foreground="White">
    <StackPanel>
        <Label Foreground="#FFFBFBFB">Call Number</Label>
        <TextBox x:Name="CallInput" />
        <Button Click="ButtonBase_OnClick" IsDefault="True">
            UnRepair
        </Button>
    </StackPanel>
</Window>
